<td valign="middle" class="row-check" style="padding-top: 2px;">
  {{check-box nodeId=model.id}}
</td>
<td data-title="{{dt.state}}" class="state">
  {{badge-state model=model}}
</td>
<td data-title="{{dt.name}}">
  <a href="{{href-to 'authenticated.project.index' model.id}}" {{action "switchTo" model.id}}>{{model.displayName}}</a>
</td>
{{#if showCluster}}
  <td data-title="{{dt.cluster}}">
    {{model.cluster.displayName}}
  </td>
{{/if}}
<td data-title="{{dt.stacks}}">
  {{model.numStacks}}
</td>
<td data-title="{{dt.services}}">
  {{model.numServices}}
</td>
<td data-title="{{dt.containers}}">
  {{model.numContainers}}
</td>
<td data-title="{{dt.default}}">
  {{#if model.isDefault}}<i class="icon icon-check"></i>{{else}}<span class="text-muted">&ndash;</span>{{/if}}
</td>
<td data-title="{{dt.actions}} "class="actions">
  {{action-menu model=model}}
</td>
